class Solution {
public:
    int mySqrt(int x) {
        long long sqrt=0;
        if(x==1)
        //if x/2 we take 1 
        return 1;
        for(long long i=0;i<=x/2;i++)
        {
            if(i*i<=x)
            sqrt=i;
            else
            break;
        }
        return sqrt;
    }
};
